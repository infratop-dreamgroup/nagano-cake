<div class=“row”>
  <h2>ログイン</h2>
  <%= flash[:notice] %>
  <p>会員の方はこちらからログイン</p>
  <table>
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <tbody>
    <tr class="field">
      <td><%= f.label "メールアドレス" %></td>
      <td><%= f.email_field :email, autofocus: true, autocomplete: "email", size: "40" %></td>
    </tr>
  
    <tr class="field">
      <td><%= f.label "パスワード" %></td>
      <td><%= f.password_field :password, autocomplete: "current-password", size: "40" %></td>
    </tr>
  
    <tr class="actions">
      <td><%= f.submit "ログイン" %></td>
    </tr>
  <% end %>
  </tbody>
  </table>
  <br><br>
  
  
  <%= render "public/shared/links" %>
</div>